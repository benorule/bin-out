      <h2>User Table</h2>
     
<table class="table">
  <thead class="thead-dark">
    <tr>
      
      <th scope="col">#</th>
 
      <th scope="col">Name</th>

      <th scope="col">Unit Number</th>
      <th scope="col">House Number</th>
      <th scope="col">Streetname</th>
      <th scope="col">Postcode</th>
      <th scope="col">Approve Signup</th>
      <th scope="col">Delete Signup</th>
     
    </tr>
  </thead>
  <tbody>
    <tr>
      
      <td>1</td>
      {{#each this}}
      <td>{{fullName}}</td>
      <td>{{unitNumber}}</td>
      <td>{{houseNumber}}</td>
      <td>{{streetName}}</td>
      <td>{{postcode}}</td>
      {{/each}} 
      <td><a class="btn btn-primary" href="#" role="button" id="acceptUser">Accept</a></td>
      <td><a class="btn btn-primary" href="#" role="button" id="deleteUser">Delete</a></td>
   
    </tr>
  </tbody>
</table>


      <h2>Employee Table</h2>
     
<table class="table employeeTable">
    <thead class="thead-dark">
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Address</th>
            <th scope="col">Postcode</th>
            <th scope="col">Approve Signup</th>
            <th scope="col">Delete Signup</th>
        </tr>
    </thead>
    <tbody>
       
        <tr>
          
            <td scope="col">1</td>
             {{#each this}}
            <td scope="col">{{employeefullName}}</td>
            <td scope="col">{{email}}</td>
            <td scope="col">{{address}}</td>
            <td scope="col">{{employeepostcode}}</td>
             {{/each}} 
            <td><a class="btn btn-primary" href="#" role="button" id="acceptEmployee">Accept</a></td>
            <td><a class="btn btn-primary" href="#" role="button" id="deleteEmployee">Delete</a></td>
       
        </tr>
       
    </tbody>
</table>
