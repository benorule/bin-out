<h2>Employee Table</h2>

    <table class="table employeeTable">
      <thead class="thead-dark">
        <tr>
          <th scope="col">Employee ID</th>
          <th scope="col">Name</th>
          <th scope="col">Email</th>
          <th scope="col">Address</th>
          <th scope="col">Postcode</th>
          <th scope="col">Approve Signup</th>
          <th scope="col">Delete Signup</th>
        </tr>
      </thead>
      <tbody>

        {{#each employee}}
        <tr>
          <td scope="col">{{dataValues.id}}</td>
          <td scope="col">{{dataValues.fullName}}</td>
          <td scope="col">{{dataValues.email}}</td>
          <td scope="col">{{dataValues.address}}</td>
          <td scope="col">{{dataValues.postcode}}</td>
          <td><a class="btn btn-primary acceptEmployee" href="/admin/employees" role="button" data-id="{{dataValues.id}}">Accept</a></td>
          <td><a class="btn btn-primary deleteEmployee" href="/admin/employees" role="button" data-id="{{dataValues.id}}">Delete</a></td>
        </tr>
        {{/each}}
      </tbody>
    </table>