<div class="container">
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <h2>User Sign Up Form</h2>
        <form class="signup">
          <div class="form-group">
            <label for="fullName">Full Name (eg:-Tony Russbell)</label>
            <input type="text" class="form-control" id="fullName" placeholder="Full Name">
          </div>
          <div class="form-group">
            <label for="unitNumber">Unit Number (eg:-5)</label>
            <input type="number" class="form-control" id="unitNumber" placeholder="Unit Number">
          </div>
          <div class="form-group">
            <label for="houseNumber">House Number (eg:-10)</label>
            <input type="number" class="form-control" id="houseNumber" placeholder="House Number">
          </div>
          <div class="form-group">
            <label for="streetName">Street Name (The Parade)</label>
            <input type="text" class="form-control" id="streetName" placeholder="Street Name">
          </div>
          <div class="form-group">
            <label for="postcode">Post Code (eg:-5082)</label>
            <input type="number" class="form-control" id="postcode" placeholder="Post Code">
          </div>

          <div class="form-group">
            <label for="postcode">Your Phone Number (61420719202)</label>
            <input type="number" class="form-control" id="phoneNumber" placeholder="phone number">
          </div>
          
          <div style="display: none" id="alert" class="alert alert-danger" role="alert">
            <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
            <span class="sr-only">Error:</span> <span class="msg"></span>
          </div>
          <button type="submit" class="btn btn-default">Sign Up</button>
        </form>
        <br />
        <p>Or User log in <a href="/user/dashboard/">here</a></p>
      </div>
    </div>
  </div>

  